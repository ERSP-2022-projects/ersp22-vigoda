#Nexus
begin mrbayes;
    execute ..\treegen\results\8187_data.nex;
    lset nst=6 rates=gamma;
    mcmc ngen=50000 samplefreq=1000 nchains=1 nruns=2 printfreq=1000 stoprule=auto stopval=0.1;
    sumt burnin=250;
    sump burnin=250;
    sumt relburnin=yes;
    sump relburnin=yes;
    set autoclose=yes nowarn=yes;
    quit;
end;
